<svg viewBox="0 0 400 400" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="400" fill="#F9F6EE"/>
  
  <!-- Gradient overlay -->
  <defs>
    <linearGradient id="grad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FFF4D9;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#F6C851;stop-opacity:0.2" />
    </linearGradient>
  </defs>
  <rect width="400" height="400" fill="url(#grad2)"/>
  
  <!-- Decorative circle -->
  <circle cx="200" cy="200" r="120" fill="none" stroke="#F6C851" stroke-width="2" opacity="0.3"/>
  
  <!-- Person + Plus icon -->
  <g transform="translate(110, 110)">
    <!-- Person circle (head) -->
    <circle cx="50" cy="35" r="18" fill="none" stroke="#F6C851" stroke-width="3"/>
    
    <!-- Person body -->
    <path d="M 35 60 Q 35 55 50 55 Q 65 55 65 60 L 65 85 Q 65 92 50 92 Q 35 92 35 85 Z" 
          fill="none" stroke="#F6C851" stroke-width="3"/>
    
    <!-- Plus sign -->
    <g opacity="0.8">
      <line x1="90" y1="35" x2="115" y2="35" stroke="#7A5230" stroke-width="3" stroke-linecap="round"/>
      <line x1="102" y1="23" x2="102" y2="48" stroke="#7A5230" stroke-width="3" stroke-linecap="round"/>
    </g>
  </g>
  
  <!-- Decorative elements -->
  <circle cx="80" cy="280" r="10" fill="#F6C851" opacity="0.4"/>
  <circle cx="300" cy="100" r="8" fill="#F6C851" opacity="0.3"/>
  <path d="M 280 320 L 300 340" stroke="#7A5230" stroke-width="3" opacity="0.3"/>
  
  <!-- Text placeholder -->
  <text x="200" y="380" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#7A5230" opacity="0.5">
    Register Illustration
  </text>
</svg>
